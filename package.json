{
  "name": "@bahaa-kallas/coingecko-api-v3",
  "version": "0.0.1",
  "description": "Client SDK library for coingecko api v3. A fork of samuraitruong/coingecko-api-v3.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "dev": "ts-node ./src/index",
    "test": "jest --runInBand --verbose --silent=false",
    "doc-gen": "rm -rf ./dist && typedoc --out ./docs src/",
    "prepublish": "pnpm run build",
    "publish-doc": "pnpm run doc-gen &&  gh-pages -d ./docs",
    "release": "pnpm run standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bahaa-kallas/coingecko-api-v3.git"
  },
  "keywords": [
    "coingecko",
    "api",
    "cryptocurrency"
  ],
  "author": "dev.bahaakallas@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahaa-kallas/coingecko-api-v3/issues"
  },
  "homepage": "https://samuraitruong.github.io/coingecko-api-v3",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.59",
    "gh-pages": "^6.0.0",
    "jest": "^29.7.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@9.5.0",
  "dependencies": {
    "https": "^1.0.0"
  },
  "pnpm": {
    "strict-peer-dependencies": true,
    "lockfile-only": true
  }
}
